{
  "parser": "babel-eslint",
  "plugins": [
      "react"
  ],
  "globals": {
    "$": true,
    "IDLE_TIMEOUT": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "modules": true,
    "spread": true,
    "templateStrings": true,
    "jsx": true
  },
  "rules": {
    "consistent-return": [0],
    "key-spacing": [0],
    "quotes": [0],
    "new-cap": [0],
    "no-multi-spaces": [0],
    "no-shadow": [0],
    "no-unused-vars": [1],
    "no-use-before-define": [2, "nofunc"],
    "no-underscore-dangle": [0],
    "camelcase": [2],
    "space-unary-ops": [1],
    "space-infix-ops": [1],
    "comma-spacing": [1],
    "no-mixed-requires": [1],
    "eqeqeq": [1],
    "comma-dangle": [1],
    "curly": [1],
    "semi-spacing": [1],
    "dot-notation": [1],
    "no-unused-expressions": [1],
    "no-extra-semi": [1],
    "semi": [1],
    "no-empty": [1]
  }
}
